{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "flowtype"
    ],
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "semi": [
            2,
            "never"
        ],
        "no-unexpected-multiline": 2,
        "comma-dangle": 0
    }
}